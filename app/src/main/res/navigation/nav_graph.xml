    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LoginFragment">

    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.kubsu.kubsuappmobile.ui.main.auth.AuthFragment"
        tools:layout="@layout/fragment_auth">
        <action
            android:id="@+id/action_LoginFragment_to_ProductsFragment"
            app:destination="@id/TimetableFragment" />
    </fragment>
    <fragment
        android:id="@+id/TimetableFragment"
        android:name="com.kubsu.kubsuappmobile.ui.main.timetable.TimetableFragment"
        tools:layout="@layout/fragment_timetable"/>
    <fragment
        android:id="@+id/accountingFragment2"
        android:name="com.kubsu.kubsuappmobile.ui.main.accounting.AccountingFragment"
        android:label="fragment_accounting"
        tools:layout="@layout/fragment_accounting" />
